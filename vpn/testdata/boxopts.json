{
	"log": {
		"disabled": true
	},
	"inbounds": [
		{
			"type": "socks",
			"tag": "socks-in",
			"listen": "127.0.0.1"
		}
	],
	"outbounds": [
		{
			"type": "direct",
			"tag": "direct"
		},
		{
			"type": "block",
			"tag": "block"
		},
		{
			"type": "http",
			"tag": "http1-out",
			"server": "127.0.0.1",
			"server_port": 4080
		},
		{
			"type": "http",
			"tag": "http2-out",
			"server": "127.0.0.1",
			"server_port": 4443
		},
		{
			"type": "socks",
			"tag": "socks1-out",
			"server": "127.0.0.1",
			"server_port": 5080
		},
		{
			"type": "socks",
			"tag": "socks2-out",
			"server": "127.0.0.1",
			"server_port": 5443
		},
		{
			"type": "mutableurltest",
			"tag": "auto-lantern",
			"outbounds": [
				"http1-out",
				"http2-out"
			]
		},
		{
			"type": "mutableurltest",
			"tag": "auto-user",
			"outbounds": [
				"socks1-out",
				"socks2-out"
			]
		},
		{
			"type": "mutableselector",
			"tag": "lantern",
			"outbounds": [
				"auto-lantern",
				"http1-out",
				"http2-out"
			]
		},
		{
			"type": "mutableselector",
			"tag": "user",
			"outbounds": [
				"auto-user",
				"socks1-out",
				"socks2-out"
			]
		},
		{
			"type": "mutableurltest",
			"tag": "auto-all",
			"outbounds": [
				"auto-lantern",
				"auto-user"
			]
		}
	],
	"route": {
		"rules": [
			{
				"clash_mode": "direct",
				"outbound": "direct"
			},
			{
				"clash_mode": "lantern",
				"outbound": "lantern"
			},
			{
				"clash_mode": "user",
				"outbound": "user"
			}
		]
	},
	"experimental": {
		"cache_file": {
			"enabled": true,
			"cache_id": "test_cache"
		},
		"clash_api": {
			"default_mode": "Rule"
		}
	}
}
